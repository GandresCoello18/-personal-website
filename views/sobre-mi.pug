    h2.text-center.mt-4.mb-4 Formacion Online
    h3.text-center.p-2 Carreras

    .row.justify-content-center
        .col-12.mb-4
            .ui.six.doubling.special.centered.cards
                each valor in diplomas
                    a.card.p-2(href=valor.enlace, target='_blank')
                        .image
                            img(src=valor.imagen_recon, alt=valor.titulo_recon)
                            h4.text-center= valor.titulo_recon
                

    
    h3.text-center.p-2 Certificados

    .row.justify-content-center
