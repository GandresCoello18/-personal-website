include head.pug
.bg-dark
    include nav.pug

.container-fluid.p-5
    .row.justify-content-center
        .col-12.col-md-8

            h2.text-center ESTATICO
            table.table.text-center
                thead
                    tr
                        th(scope='col') Progreso
                        th(scope='col') .
                        th(scope='col') .
                        th(scope='col') Tech
                tbody
                    tr
                        th(colspan='3') 
                            #uno.ui.active.progress(data-percent='84')
                                .bar
                                    .label.text-white 84% HTML 5
                        td
